{
  "family": "lambda-processing-task",
  "networkMode": "awsvpc",
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "256",
  "memory": "512",
  "executionRoleArn": "arn:aws:iam::756107513788:role/ecsTaskExecutionRole",
  "taskRoleArn": "arn:aws:iam::756107513788:role/ecsTaskRoleForS3",
  "containerDefinitions": [
    {
      "name": "lambda-processing-container",
      "image": "756107513788.dkr.ecr.ap-south-1.amazonaws.com/lambda-processing-repo:latest",
      "essential": true,
      "portMappings": [
        {
          "containerPort": 80,
          "hostPort": 80,
          "protocol": "tcp"
        }
      ],
      "logConfiguration": {
            "logDriver": "awslogs",
            "options": {
                "awslogs-create-group": "true",
                "awslogs-group": "/ecs/lambda-processing-task",
                "awslogs-region": "ap-south-1",
                "awslogs-stream-prefix": "lambda-processing"
            }
        },
      "environment": [
        {
          "name": "S3_BUCKET",
          "value": "shashwatbucketunique"
        },
        {
          "name": "BUCKET2",
          "value": "shashwatbucketunique"
        },
        {
          "name": "INPUT_KEY",
          "value": "Effects-of-COVID-19-on-trade-1-February-16-December-2020-provisional.csv"
        }
      ]
    }
  ]
}